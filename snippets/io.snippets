snippet in_1 "in 1" b
${1:int} ${2:N};
cin >> $2;
endsnippet
snippet in_1_vec "in 1 + vec" b
${1:int} ${2:N};
cin >> $2;
vector<${3:int}> ${4:A}(${5:$2});
for (int ${6:i} = ${7:0}; $6 <${8:$5}; $6++) {
	cin >> $4[$6];
}
endsnippet
snippet in_1_q "in 1 + q" b
${1:int} ${2:Q};
cin >> $2;
while ($2--) {
	${3}
}
endsnippet
snippet in_2 "in 2" b
