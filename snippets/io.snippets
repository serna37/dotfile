snippet vec_in_resize "vec in resize" b
vector<vector<${1:int}>> ${2:A}(${3:N});
for (int ${4:i} = ${5:0}; $4 <${6:$3}; $4++) {
	${7:int} ${8:a};
	cin >> $8;
	$2[$4].resize($8);
	for (int ${9:j} = 0; $9 < $8; $9++) {
		cin >> $2[$4][$9];
	}
