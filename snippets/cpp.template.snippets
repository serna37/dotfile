snippet split "split" b
auto split = [](string s, char c) -> vector<string> {
	vector<string> S;
	string t;
	for (char v : s) {
		if (v == c) {
			if (!t.empty()) S.push_back(t);
			t.clear();
		} else {
			t += v;
		}
	}
	if (!t.empty()) S.push_back(t);
	return S;
};
endsnippet
snippet finds "string find all pos" b
auto finds = [](const string &T, const string &S) -> vector<int> {
	vector<int> pos;
	auto p = T.find(S);
	while (p != string::npos) {
		pos.push_back(p);
		p = T.find(S, p + 1);
	}
	return pos;
};
endsnippet

