	${4}
}
endsnippet
snippet forij2 "for ij 2 loop" b
for (int ${1:i} = ${2:0}; $1 <${3:H}; ++$1) {
	for (int ${4:j} = ${5:0}; $4 <${6:W}; ++$4) {
		${7}
	}
}
endsnippet
snippet fori_re "for i reverse" b
// 反転
